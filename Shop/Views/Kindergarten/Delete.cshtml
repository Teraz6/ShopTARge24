@model Shop.Models.Kindergarten.KindergartenDeleteViewModel

<h1>Delete</h1>

<div>
	<h4>Kindergarten</h4>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="Id" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.Id)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="GroupName" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.GroupName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="ChidlrenCount" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.ChidlrenCount)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="KindergartenName" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.KindergartenName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="TeacherName" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.TeacherName)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.CreatedAt)
		</div>
	</dl>
	<dl class="row">
		<div class="form-group row">
			<label asp-for="UpdatedAt" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.UpdatedAt)
		</div>
	</dl>

	<dl>
		@foreach (var images in Model.Images)
		{
			<partial name="_Images" model="images" />
		}
	</dl>
	<form asp-action="DeleteConfirmation">
		<input type="hidden" asp-for="Id" />
		<input type="submit" value="Delete" class="btn btn-danger" />
	</form>
	<a asp-action="Index" asp-controller="kindergarten" class="btn btn-success">Back</a>

</div>